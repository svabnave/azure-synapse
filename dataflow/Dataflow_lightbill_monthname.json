{
	"name": "Dataflow_lightbill_monthname",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Excel1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "LS_ADL_mysyntest",
						"type": "LinkedServiceReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Time as string,",
				"          Forbruk as double,",
				"          Pris as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(Month = month(toDate(left(Time,10),'yyyy-MM-dd'))\r",
				",",
				"          Time = toDate(left(Time,10),'yyyy-MM-dd'),",
				"          KW_Consumption = Forbruk,",
				"          Price_per_kw = Pris,",
				"          Price = Forbruk*Pris,",
				"          Year = \r",
				"year(toDate(left(Time,10),'yyyy-MM-dd'))\r",
				"\r",
				",",
				"          Month_Name = month(toDate(left(Time,10),'yyyy-MM-dd'))) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          Month,",
				"          Year,",
				"          Date = Time,",
				"          KW_Consumption,",
				"          Price_per_kw,",
				"          Price",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 aggregate(groupBy(Year),",
				"     Total_KW_Consumption = sum(KW_Consumption),",
				"          Total_Price = sum(Price)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'staging',",
				"     folderPath: 'Agva/Yearly_bill',",
				"     overwrite: true,",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}